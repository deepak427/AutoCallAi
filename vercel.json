{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@now/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "build": {
      "env": {
        "PYTHONUSERBASE": "./python"
      },
      "functions": {
        "postBuild": "pip install -r requirements.txt -t ./python/lib/python3.9/site-packages"
      }
    }
  }
  